{
  "name": "@bluecadet/orbit-react",
  "version": "0.2.1",
  "sideEffects": false,
  "license": "MIT",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    "./orbit-parallax": {
      "types": "./dist/orbit-parallax.d.ts",
      "import": "./dist/orbit-parallax.js",
      "require": "./dist/orbit-parallax.cjs"
    },
    "./orbit-motion-toggle": {
      "types": "./dist/orbit-motion-toggle.d.ts",
      "import": "./dist/orbit-motion-toggle.js",
      "require": "./dist/orbit-motion-toggle.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "git clean -xdf .turbo node_modules dist .cache",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@repo/eslint": "*",
    "@repo/prettier": "*",
    "@repo/typescript": "*",
    "eslint": "^9.19.0",
    "prettier": "^3.4.2",
    "tsup": "^8.0.2",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@bluecadet/orbit-vanilla": "0.2.1",
    "@lit/react": "^1.0.6",
    "lit": "^3.2.1"
  },
  "peerDependencies": {
    "motion": "^12.0.6",
    "react": "^18.3.1 || ^19.0.0"
  },
  "prettier": "@repo/prettier",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluecadet/orbit.git"
  },
  "author": "Bluecadet",
  "bugs": {
    "url": "https://github.com/bluecadet/orbit/issues"
  },
  "homepage": "https://github.com/bluecadet/orbit/blob/main/packages/orbit-react#readme"
}
