    "tsup": "^8.0.2",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@bluecadet/orbit-vanilla": "0.0.0",
    "@lit/react": "^1.0.6"
  },
  "peerDependencies": {
    "react": "^18.3.1 || ^19.0.0"
  },
  "prettier": "@repo/prettier"
}
