{
  "name": "embla-carousel-a11y",
  "version": "0.0.0",
  "description": "A11y Plugin for Embla Carousel",
  "main": "index.js",
  "sideEffects": false,
  "license": "MIT",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    "./index": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "git clean -xdf .turbo node_modules dist .cache",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@repo/eslint": "*",
    "@repo/prettier": "*",
    "@repo/typescript": "*",
    "eslint": "^9.19.0",
    "prettier": "^3.4.2",
    "tsup": "^8.0.2",
    "typescript": "^5.7.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluecadet/orbit.git"
  },
  "author": "Bluecadet",
  "bugs": {
    "url": "https://github.com/bluecadet/orbit/issues"
  },
  "homepage": "https://github.com/bluecadet/orbit/blob/main/packages/embla-carousel-a11y#readme"
}
